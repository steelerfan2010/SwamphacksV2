drop table RegularNumbers;
drop table PowerballNumbers;
drop table Multiplier;

create table RegularNumbers (
	theDate date,
	regularNumber int
);

create table PowerballNumbers (
	theDate date,
	powerballNumber int
);

create table Multiplier (
	theDate date,
	multiplier int
);

SELECT SESSION_ID FROM DBA_DML_LOCKS WHERE NAME = Table RegularNumbers;
